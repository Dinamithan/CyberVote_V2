      PROCEDURE MaFenêtre()
   -
     code : |1+
      
      REQ_NomCandidat.ParamCodeSalarie = nNuméroCodeElecteur
      HExécuteRequête(REQ_NomCandidat)
      Si HLitPremier(REQ_NomCandidat) ALORS
      	LIB_Bonjour = " " +  LIB_Bonjour + " " +  REQ_NomCandidat.NomSalarie +" " + REQ_NomCandidat.PrenomSalarie
      	
      FIN
      
      
      SI bVote_Nul = Vrai ALORS
      	LIB_Confirmation = "Confirmer de soumettre un vote blanc."
      	
      SINON
      	REQ_NomCandidat.ParamCodeSalarie = nNuméroCodeElecteur
      	HExécuteRequête(REQ_NomCandidat)
      	
      	SI HLitPremier(REQ_NomCandidat) ALORS
      		
      		//LIB_Bonjour = " " +  LIB_Bonjour + " " +  REQ_NomCandidat.NomSalarie +" " + REQ_NomCandidat.PrenomSalarie
      		LIB_Confirmation = LIB_Confirmation + sNomPrenomCandidat
      		
      	FIN
      FIN
      
      
      
